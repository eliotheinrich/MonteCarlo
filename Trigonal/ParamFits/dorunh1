g++ -O3 -std=c++11 -I ~/lib HysteresisCurve.cpp -o output

for L in 2 3 4 5 6 7 10 15; do
    foldername=hLL_"$L";
    paramsname="$foldername/params.txt"
    mkdir "data/$foldername";
    ./make_params "data/$paramsname" 25 "$L" 1. 0.2 3.5 0.5 0. 0. 0. 100 10 8000 60 60;
    ./output "data/$foldername" "data/$paramsname" & 
done
rm output
wait
