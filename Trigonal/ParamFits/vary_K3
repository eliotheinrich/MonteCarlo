g++ -I ~/lib -pthread -O3 -std=c++11 -I ~/lib SusceptibilityCurve.cpp -o outputK3

for K3 in 0.01 0.1 0.2 1.0  
    foldername=sK3_"$K3";
    paramsname="$foldername/params.txt"
    rm -r "data/$foldername"
    mkdir "data/$foldername";
    ./make_params "data/$paramsname" 32 8 0.64 0.05 2.56 0. $K3 0. 0. 0. 80 500 1000 1 1;
    ./outputJ "data/$foldername" "data/$paramsname" 0.41 4

rm outputK1
