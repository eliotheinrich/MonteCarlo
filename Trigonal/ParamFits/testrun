g++ -O3 -std=c++11 -I ~/lib MagnetizationCurve.cpp -o outputtest

for H in 0.05 0.1 0.2 0.3 0.5 0.6 1.0 2.0; do
    foldername=mTest"$H"
    paramsname="$foldername/params.txt"
    rm -r "data/$foldername";
    mkdir "data/$foldername";
    ./make_params "data/$paramsname" 3 3 1. 0. 1.0 0.0 "$H" 1.0 0. 0. 0. 40 50 10000 50 50;
    ./outputtest "data/$foldername" "data/$paramsname" 0.02 & 
done
sleep 3
rm outputtest
wait

