g++ -I ~/lib -pthread -O3 -std=c++11 -I ~/lib SusceptibilityCurve.cpp -o outputJ

for J in 0.3 1.0 2.0 
do
    foldername=sJ2_"$J";
    paramsname="$foldername/params.txt"
    rm -r "data/$foldername"
    mkdir "data/$foldername";
    ./make_params "data/$paramsname" 32 8 $J 0.05 2.56 0. 0.15 0. 0.2 0. 80 5000 1000 10 1;
    ./outputJ "data/$foldername" "data/$paramsname" 0.41 4
done

rm outputJ
